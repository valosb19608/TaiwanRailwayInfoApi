<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.railway.info.model.Station" table="station" schema="taiwan_railway_api_info">
        <id name="stationUid" column="station_uid"/>
        <property name="stationId" column="station_id"/>
        <property name="stationNameTw" column="station_name_tw"/>
        <property name="stationNameEn" column="station_name_en"/>
        <property name="positionLat" column="position_lat"/>
        <property name="positionLon" column="position_lon"/>
        <property name="stationAddress" column="station_address"/>
        <property name="stationPhone" column="station_phone"/>
        <property name="stationClass" column="station_class"/>
        <property name="stationUrl" column="station_url"/>
        <property name="modifyTime" column="modify_time"/>
    </class>
</hibernate-mapping>